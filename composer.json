{
    "name": "csabourin/craft-stamp-passport",
    "description": "A self-contained Craft CMS 4 plugin for bilingual bucket list / passport experiences with QR code check-ins.",
    "version": "1.0.0",
    "type": "craft-plugin",
    "license": "proprietary",
    "keywords": [
        "craftcms",
        "craft-plugin",
        "passport",
        "stamp",
        "qr-code",
        "geofence"
    ],
    "require": {
        "php": "^8.2",
        "craftcms/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "csabourin\\stamppassport\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/csabourin/cottagePassport/issues"
    },
    "extra": {
        "handle": "stamp-passport",
        "name": "Stamp Passport",
        "developer": "Christian Sabourin",
        "schemaVersion": "1.0.0",
        "class": "csabourin\\stamppassport\\Plugin",
        "hasCpSettings": true,
        "hasCpSection": true
    }
}
